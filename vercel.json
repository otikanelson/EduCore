{
  "version": 2,
  "builds": [
    {
      "src": "educore-os/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "educore-os/frontend/dist"
      }
    },
    {
      "src": "educore-os/backend/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/educore-os/backend/api/index.ts"
    }
  ]
}
